V_p_allocation <- (1- 450/30000)*(30000/90000)*(.45*(1-.45)/(450-1)) + #house 

(1- 413/55000)*(55000/90000)*(.25*(1-.25)/(413-1)) + #apt


(1- 37/5000)*(5000/90000)*(.03*(1-.03)/(37-1)) #condo


p_srs <- (1/3)*.45 + (55000/90000)*.25 + (5000/90000)*(.03)
(1-900/90000)*(p_srs - (1-p_srs)/(900-1))

V_p_srs  <- (1-900/90000)*(p_srs * (1-p_srs)/(900-1))

V_p_allocation/V_p_srs
